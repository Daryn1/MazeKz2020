@model List<HeroViewModel>
<h1>Heroes list: </h1>
@foreach(var hero in Model)
{
<div class="heroes">
    <h1>@hero.Name</h1>
    <h1>HP - @hero.HP</h1>
    <h2>Mana - @hero.Mana</h2>
    <h3>Properties: 
    <ul>
    @foreach (var prop in hero.Properties)
    {
        <li class="property">@prop</li>
    }
    </ul>
    </h3>
    
</div>
}